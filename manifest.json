{
  "manifest_version": 3,
  "name": "Subway Surfers",
  "description": "This extension will play SubwaySurfers in the background to help you focus!",
  "version": "1.0.0",
  "icons": {
    "128": "128.png"
    },

  "author": "Aaditya Doiphode & Jagannath Prabhakaran",
  "permissions": ["activeTab", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["SubwaySurfers.mp4"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_title": "Subway Surfers"
  }
}
